<h1><b>Books</b></h1>
<body>
<table>
  <tr>
    <th>Title</th>
    <th>Body</th>
  </tr>
  <td>リーダブルコード</td>
  <td>より良いコードを書くためのシンプルで実践的なテクニック</td>
  <td>show</td>
  <td>edit</td>
  <td>destroy</td>
</table>
</body>

<h2><b>New book</b></h2>

<%= form_with model:@book,local:true do |f| %>

<div class="field">
<h4>Title</h4>
<%= f.text_field :title %>
</div>

<div class="field">
<h4>Body</h4>
<%= f.text_area :body %>
</div>

<div class="actions">
<%= f.submit 'Create Book' %>
</div>


<% end %>